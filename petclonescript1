local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Spawnerロード
local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()

-- Kavo UIライブラリ読み込み
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Custom Pet Spawner", "Midnight")

-- タブとセクション作成
local PetTab = Window:NewTab("Pets")
local PetSection = PetTab:NewSection("Spawn Fennec Fox")

-- スポーンボタン追加
PetSection:NewButton("Spawn Fennec Fox (5kg, Age12)", "Spawn a Fennec Fox pet", function()
	Spawner.SpawnPet("Fennec Fox", 5.0, 12)
end)
