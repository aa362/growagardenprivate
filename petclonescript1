local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local player = Players.LocalPlayer
local detected = {}

-- ãƒã‚§ãƒƒã‚¯é–¢æ•°
local function scanFennec()
	for _, model in ipairs(workspace:GetDescendants()) do
		if model:IsA("Model") and model.Name:lower():find("fennec") and not detected[model] then
			detected[model] = true

			-- ãƒ¢ãƒ‡ãƒ«åã®é€šçŸ¥
			StarterGui:SetCore("ChatMakeSystemMessage", {
				Text = "ğŸ¦Š Fennec Fox ã‚’æ¤œå‡º: " .. model:GetFullName(),
				Color = Color3.fromRGB(255, 200, 0),
				Font = Enum.Font.SourceSansBold,
				FontSize = Enum.FontSize.Size24
			})

			-- å«ã¾ã‚Œã‚‹ MeshPart ã‚’èª¿æŸ»
			for _, part in ipairs(model:GetDescendants()) do
				if part:IsA("MeshPart") then
					local msg = "[MeshPart] " .. part.Name ..
						"\nMeshId: " .. part.MeshId ..
						"\nTextureId: " .. part.TextureID

					StarterGui:SetCore("ChatMakeSystemMessage", {
						Text = msg,
						Color = Color3.fromRGB(100, 255, 100),
						Font = Enum.Font.SourceSans,
						FontSize = Enum.FontSize.Size24
					})
				end
			end
		end
	end
end

-- ğŸ”„ 5ç§’ã”ã¨ã«ç›£è¦–ãƒ«ãƒ¼ãƒ—
task.spawn(function()
	while true do
		scanFennec()
		task.wait(5) -- 5ç§’å¾…æ©Ÿ
	end
end)
